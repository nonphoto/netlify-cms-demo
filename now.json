{
    "version": 2,
    "alias": [
        "netlify-cms-demo.now.sh"
    ],
    "builds": [
        { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "build" }},
        { "src": "admin/*", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/config.yml", "dest": "/admin/config.yml" }
    ]
}
